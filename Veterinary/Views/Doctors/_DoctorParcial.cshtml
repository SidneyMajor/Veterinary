@model Veterinary.Data.Entities.Doctor


    <div class="col-md-12">
        <div class="form-group">
            <ejs-textbox ejs-for="FullName" id="fullname" type="text" placeholder="Full Name" floatLabelType="Always" enabled="false"></ejs-textbox>
        </div>
    </div>

    <div class="col-md-6">
        <div class="form-group">
            <ejs-textbox ejs-for="Nationality" id="nationality" placeholder="Nationality" type="text" floatLabelType="Always" enabled="false"></ejs-textbox>
        </div>
    </div>

    <div class="col-md-6">
        <div class="form-group">
            <h3>Gender</h3>
            <ejs-radiobutton ejs-for="Gender" id="radio1" label="M" value="M" disabled="true"></ejs-radiobutton>
            <ejs-radiobutton ejs-for="Gender" id="radio2" label="F" value="F" disabled="true"></ejs-radiobutton>
        </div>
    </div>

    <div class="col-md-12">
        <div class="form-group">
            <ejs-textbox ejs-for="Address" id="address" placeholder="Address" type="text" floatLabelType="Always" enabled="false"></ejs-textbox>
        </div>
    </div>

    <div class="col-md-6">
        <div class="form-group">
            <ejs-textbox ejs-for="ZipCode" id="zipcode" placeholder="Zip Code" type="text" floatLabelType="Always" enabled="false"></ejs-textbox>
        </div>
    </div>

    <div class="col-md-6">
        <div class="form-group">
            <ejs-textbox ejs-for="TaxNumber" id="taxnumber" placeholder="Tax Number" type="text" floatLabelType="Always" enabled="false"></ejs-textbox>
        </div>
    </div>

    <div class="col-md-6">
        <div class="form-group">
            <ejs-textbox ejs-for="SSNumber" id="ssnumber" placeholder="Social Security Number" type="text" floatLabelType="Always" enabled="false"></ejs-textbox>
        </div>
    </div>

    <div class="col-md-6">
        <div class="form-group">
            <ejs-textbox id="Specialty" ejs-for="Specialty.Description" placeholder="Specialty" type="text" floatLabelType="Always" enabled="false">
            </ejs-textbox>
        </div>
    </div>

    <div class="col-md-6">
        <div class="form-group">
            <ejs-textbox id="DocumentType" ejs-for="DocumentType.Description" placeholder="Document" type="text" floatLabelType="Always" enabled="false">
            </ejs-textbox>
        </div>
    </div>

    <div class="col-md-6">
        <div class="form-group">
            <ejs-textbox ejs-for="Document" id="document" placeholder="Nº Document" type="text" floatLabelType="Always" enabled="false"></ejs-textbox>
        </div>
    </div>

    <div class="col-md-6">
        <div class="form-group">
            <ejs-textbox ejs-for="PhoneNumber" id="phoneNumber" placeholder="Phone Number" type="text" floatLabelType="Always" enabled="false"></ejs-textbox>
        </div>
    </div>

    <div class="col-md-6">
        <div class="datepicker">
            <ejs-datepicker ejs-for="DateOfBirth" format="dd-MM-yyyy" id="datetimepicker" placeholder="Date Of Birth" floatLabelType="Always" enabled="false"> </ejs-datepicker>
        </div>
    </div>

    