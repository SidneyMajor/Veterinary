        <link rel="stylesheet" href="~/css/flaticon.css">
        <link rel="stylesheet" href="~/css/style.css">
        <link href="~/css/site.css" rel="stylesheet" />
        <!-- Syncfusion Essential JS 2 Styles -->
        <link rel="stylesheet" href="https://cdn.syncfusion.com/ej2/material.css" />
        <!-- Data Table -->
        <link rel="stylesheet" href="https://cdn.datatables.net/1.10.21/css/jquery.dataTables.min.css" />
        <link href="~/css/adminlte.min.css" rel="stylesheet" />


    </environment>




</head>

<body>
    <div class="card-header badge-primary wrap">

        <div class="container">
            <div class="row ">

                <div class="col-md-12 d-lg-flex justify-content-end ">
                    @if (this.User.Identity.IsAuthenticated)
                    {
                        <a asp-area="" asp-controller="Account" asp-action="ChangeUser" style="color:white">@this.User.Identity.Name</a>
                        <a asp-area="" asp-controller="Account" asp-action="Logout" style="color:white; padding-left:40px">Logout</a>
                    }
                    else
                    {
                        <a asp-area="" asp-controller="Account" asp-action="Login" style="color:white">Login</a>
                    }
                </div>

            </div>
        </div>

    </div>

    <nav class="navbar navbar-expand-lg  ftco_navbar  ftco-navbar-light " id="ftco-navbar">
        <div class="container ">
            <a class="navbar-brand" asp-area="" asp-controller="Home" asp-action="Index"><span class="flaticon-pawprint-1 mr-2"></span>Veterinary</a>
            <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#ftco-nav" aria-controls="ftco-nav" aria-expanded="false" aria-label="Toggle navigation">
                <span class="fa fa-bars"></span> Menu
            </button>
            <div class="collapse navbar-collapse" id="ftco-nav">
                <ul class="navbar-nav ml-auto">
                    <li class="nav-item active"><a asp-area="" asp-controller="Home" asp-action="Index" class="nav-link">Home</a></li>
                    <li class="nav-item "><a asp-area="" asp-controller="Home" asp-action="About" class="nav-link">About</a></li>
                    <li class="nav-item "><a asp-area="" asp-controller="Home" asp-action="Contact" class="nav-link">Contact</a></li>
                    @if (this.User.Identity.IsAuthenticated && this.User.IsInRole("Admin"))
                    {
                        <li class="nav-item"><a asp-area="" asp-controller="Admin" asp-action="Index" class="nav-link">DashBoard</a></li>

                        <!----<li class="nav-item"><a asp-area="" asp-controller="Admin" asp-action="ListClient" class="nav-link">Clients</a></li>
                        <li class="nav-item"><a asp-area="" asp-controller="DocumentTypes" asp-action="Index" class="nav-link">Documents</a></li>
                        <li class="nav-item"><a asp-area="" asp-controller="Species" asp-action="Index" class="nav-link">Species</a></li>
                        <li class="nav-item"><a asp-area="" asp-controller="Specialties" asp-action="Index" class="nav-link">Specialties</a></li>
                        <li class="nav-item"><a asp-area="" asp-controller="Doctors" asp-action="ListDoctor" class="nav-link">Doctors</a></li>
                        <li class="nav-item"><a asp-area="" asp-controller="Appointments" asp-action="Index" class="nav-link">Appointments</a></li>
                        <li class="nav-item"><a asp-area="" asp-controller="Admin" asp-action="ListAnimal" class="nav-link">Animals</a></li>-->

                    }

                    @if (this.User.Identity.IsAuthenticated && this.User.IsInRole("Doctor"))
                    {
                        <li class="nav-item"><a asp-area="" asp-controller="Doctors" asp-action="MyAppointment" class="nav-link">My Appointments</a></li>
                    }

                    @if (this.User.Identity.IsAuthenticated && this.User.IsInRole("Customer"))
                    {
                        <li class="nav-item"><a asp-area="" asp-controller="Animals" asp-action="Index" class="nav-link">Animals</a></li>

                    }

                    @if (this.User.Identity.IsAuthenticated)
                    {
                        <li class="nav-item"><a asp-area="" asp-controller="Appointments" asp-action="Index" class="nav-link">Appointments</a></li>
                    }



                </ul>
            </div>
        </div>
    </nav>
    <!-- END nav -->


    <partial name="_CookieConsentPartial" />




    @RenderBody()

    <environment include="Development">
        <script src='https://kit.fontawesome.com/a076d05399.js'></script>
        <script src="~/js/jquery.min.js"></script>
        <script src="~/js/jquery-migrate-3.0.1.min.js"></script>
